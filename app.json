{
  "expo": {
    "name": "Medium Unlocker",
    "slug": "medium_unlocker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1a1c24"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "mediumunlocker"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#1a1c24"
      },
      "package": "com.akashkaushik.medium_unlocker"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router", "expo-share-intent"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "build": {
          "experimental": {
            "ios": {
              "appExtensions": []
            }
          }
        },
        "projectId": "3a5eb1ba-d986-47c5-bd49-2e714dcd4696"
      }
    },
    "owner": "TheMythologist",
    "githubUrl": "https://github.com/TheMythologist/medium_unlocker"
  }
}
